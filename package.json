		"globals": "^15.11.0",
		"npm-check-updates": "^17.1.4",
		"prettier": "^3.3.3",
		"prettier-plugin-svelte": "^3.2.7",
		"typescript-eslint": "^8.10.0"
	},
	"type": "module",
	"dependencies": {
		"@sveltejs/adapter-static": "^3.0.5",
		"@sveltejs/kit": "^2.7.2",
		"@sveltejs/vite-plugin-svelte": "^4.0.0",
